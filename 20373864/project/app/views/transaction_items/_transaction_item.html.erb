<div id="<%= dom_id transaction_item %>">
  <p>
    <strong>User:</strong>
    <%= transaction_item.user_id %>
  </p>

  <p>
    <strong>Product:</strong>
    <%= transaction_item.product_id %>
  </p>

  <p>
    <strong>Quantity:</strong>
    <%= transaction_item.quantity %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= transaction_item.price %>
  </p>

  <p>
    <strong>Transaction order:</strong>
    <%= transaction_item.transaction_order_id %>
  </p>

</div>

<div id="<%= dom_id cart_item %>">
  <p>
    <strong>User:</strong>
    <%= cart_item.user_id %>
  </p>

  <p>
    <strong>Product:</strong>
    <%= cart_item.product_id %>
  </p>

  <p>
    <strong>Quantity:</strong>
    <%= cart_item.quantity %>
  </p>

  <p>
    <strong>Sum price:</strong>
    <%= cart_item.sum_price %>
  </p>

  <p>
    <strong>Added time:</strong>
    <%= cart_item.added_time %>
  </p>

</div>
